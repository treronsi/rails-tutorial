<!Doctype html>
<html>
 <h1> task</h1>
 <table>
  <thead>
  	<tr>
  		<th>task name</th>
  	</tr>
  </thead>
  <tbody>
  	<% @tasks.each do |task| %>
  	<tr>
  		<td> <%= task.title %> </td>
  		<td><%= link_to 'edit',edit_task_path(task) %></td>
  		<td><%= link_to 'delete',task_path(task), method: :delete %></td>
  	</tr>
  	<%end%>
  </tbody>
 </table>
 <%= link_to 'add' , new_task_path %>